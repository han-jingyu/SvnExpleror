{header%header}
<table id="repos-list" class="bordered">
  <thead>
    <tr>
      <th class="name sort" rowspan="2" onclick="sortTable('repos-list', 0, 'link', this)">名称</th>
      <th class="realm sort" rowspan="2" onclick="sortTable('repos-list', 1, 'text', this)">认证名</th>
      <th class="lastchanged" colspan="3">最后修改</th>
      <th class="actions" rowspan="2">操作</th>
    </tr>
    <tr>
      <th class="rev sort" onclick="sortTable('repos-list', 2, 'revision', this)">版本</th>
      <th class="author sort" onclick="sortTable('repos-list', 3, 'text', this)">作者</th>
      <th class="date sort" onclick="sortTable('repos-list', 4, 'date', this)">时间</th>
    </tr>
  </thead>
  <tbody>
    {repos-list#repos-entry}
  </tbody>
</table>
{footer%footer}
