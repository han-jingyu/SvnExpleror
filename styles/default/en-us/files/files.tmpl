{header%header}
{path-overview%path-overview}
<br />
<a name="files"></a>
<table id="files-list" class="bordered">
  <thead>
    <tr class="up" onclick="foldTable('files-list');"><th colspan="6">Files ({files-count:})</th></tr>
    <tr>
      <th class="name" onclick="sortTable('files-list', 0, 'link')">Name</th>
      <th class="size" onclick="sortTable('files-list', 1, 'size')">Size</th>
      <th class="date" onclick="sortTable('files-list', 2, 'date')">Date</th>
      <th class="rev" onclick="sortTable('files-list', 3, 'revision')">Revision</th>
      <th class="author" onclick="sortTable('files-list', 4, 'text')">Author</th>
      <th class="actions">Actions</th>
    </tr>
  </thead>
  <tbody>
    {files-list#files-entry}
  </tbody>
</table>
<br />
{props-list%props-list}
{footer%footer}
